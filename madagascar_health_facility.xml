<?xml version="1.0" encoding="UTF-8"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
    author="Dolly Andriatsiferana"
    version="1.0"
    shortdescription="Preset for Madagascar specific health facilities">
    <group name="Etablissement Sanitaire" icon="presets/health/hospital.svg" >
        <group name="Etablissement Sanitaire Public" icon="presets/health/hospital.svg" >
            <item name="Site Communautaire" icon="presets/social_facility/outreach.svg" >
                <label text="Site Communautaire / Agent Communautaire" />
                <label text="---------------------------" />
                <key key="amenity" value="health_post" />
                <key key="healthcare" value="community_health_worker" />
                <key key="operator:type" value="community" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <label text="---------------------------" />
            </item>
            <item name="Case de Santé" icon="presets/health/pharmacy.svg" >
                <label text="Case de Santé" />
                <label text="---------------------------" />
                <key key="amenity" value="health_post" />
                <combo key="healthcare" text="Responsable" values="nurse, midwife" display_values="Infirmier (ou infirmière), Sage-femme" default="nurse" />
                <key key="operator:type" value="public" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <label text="---------------------------" />
            </item>
            <item name="CSB I" icon="presets/health/pharmacy.svg">
                <label text="Centre de Santé de Base Niveau I" />
                <label text="---------------------------" />
                <key key="amenity" value="health_post" />
                <combo key="healthcare" text="Responsable" values="nurse, midwife" display_values="Infirmier (ou infirmière), Sage-femme" default="nurse" />
                <key key="health_facility:MG" value="CSB1" />
                <key key="operator:type" value="public" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <label text="---------------------------" />
            </item>
            <item name="CSB II" icon="presets/health/pharmacy.svg">
                <label text="Centre de Santé de Base Niveau II" />
                <label text="---------------------------" />
                <key key="amenity" value="doctors" />
                <combo key="healthcare" text="Responsable" values="doctor, nurse, midwife" display_values="Médecin, Infirmier (ou infirmière), Sage-femme" default="doctor" />
                <key key="health_facility:MG" value="CSB2" />
                <key key="operator:type" value="public" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <label text="---------------------------" />
            </item>
            <item name="Hôpital Public (CHRD, CHRR, CHU)" icon="presets/health/hospital.svg">
                <label text="Hôpital Public" />
                <label text="---------------------------" />
                <key key="amenity" value="hospital" />
                <key key="healthcare" value="hospital" />
                <key key="operator:type" value="public" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <combo key="health_facility:MG" text="Niveau" values="CHRD, CHRR, CHU" display_values="Centre Hospitalier de Référence de District, Centre Hospitalier de Référence Régionale, Centre Hospitalier Universitaire"/>
                <label text="---------------------------" />
            </item>
        </group>
        <group name="Etablissement Sanitaire Privé"  icon="presets/health/clinic.svg">
            <item name="Poste d'accouchement" icon="presets/health/doctors.svg">
                <label text="Poste d'accouchement" />
                <label text="---------------------------" />
                <key key="amenity" value="health_post" />
                <key key="healthcare" value="midwife" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <text key="operator" text="Nom de l'opérateur" default="" delete_if_empty="true" />
                <combo key="operator:type" text="Type d'opérateur" values="private, religious, ngo, consortium, cooperative" display_values="Privé, Réligieux, ONG, Consortium, Coopérative" />
            </item>
            <item name="Poste d'infirmerie" icon="presets/health/doctors.svg">
                <label text="Poste d'infirmerie" />
                <label text="---------------------------" />
                <key key="amenity" value="health_post" />
                <key key="healthcare" value="nurse" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <text key="operator" text="Nom de l'opérateur" default="" delete_if_empty="true" />
                <combo key="operator:type" text="Type d'opérateur" values="private, religious, ngo, consortium, cooperative" display_values="Privé, Réligieux, ONG, Consortium, Coopérative" />
            </item>
            <item name="Dispensaire ou Centre de Santé de Base Privé" icon="presets/health/doctors.svg">
                <label text="Dispensaire ou Centre de Santé de Base Privé" />
                <label text="---------------------------" />
                <key key="amenity" value="health_post" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <combo key="healthcare" text="Tenu par un" values="doctor, nurse" display_values="Médecin, Infirmier (ou infirmière)" />
                <text key="operator" text="Nom de l'opérateur" default="" delete_if_empty="true" />
                <combo key="operator:type" text="Type d'opérateur" values="private, religious, ngo, consortium, cooperative" display_values="Privé, Réligieux, ONG, Consortium, Coopérative" />
            </item>
            <item name="Cabinet Médical" icon="presets/health/doctors.svg">
                <label text="Cabinet Médical" />
                <label text="---------------------------" />
                <key key="amenity" value="doctors" />
                <key key="healthcare" value="doctor" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <text key="operator" text="Nom de l'opérateur" default="" delete_if_empty="true" />
                <combo key="operator:type" text="Type d'opérateur" values="private, religious, ngo, consortium, cooperative" display_values="Privé, Réligieux, ONG, Consortium, Coopérative" />
            </item>
            <item name="Clinique ou Polyclinique"  icon="presets/health/clinic.svg">
                <label text="Clinique ou Polyclinique" />
                <label text="---------------------------" />
                <key key="amenity" value="clinic" />
                <key key="healthcare" value="clinic" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <text key="operator" text="Nom de l'opérateur" default="" delete_if_empty="true" />
                <combo key="operator:type" text="Type d'opérateur" values="private, religious, ngo, consortium, cooperative" display_values="Privé, Réligieux, ONG, Consortium, Coopérative" />
            </item>
            <item name="Hôpital Privé" icon="presets/health/hospital.svg">
                <label text="Hôpital Privé" />
                <label text="---------------------------" />
                <key key="amenity" value="hospital" />
                <key key="healthcare" value="hospital" />
                <text key="name" text="Nom de l'établissement" default="" delete_if_empty="true" />
                <text key="operator" text="Nom de l'opérateur" default="" delete_if_empty="true" />
                <combo key="operator:type" text="Type d'opérateur" values="private, religious, ngo, consortium, cooperative" display_values="Privé, Réligieux, ONG, Consortium, Coopérative" />
            </item>
        </group>
    </group>
</presets>
